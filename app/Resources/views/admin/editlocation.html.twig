{% extends 'base.html.twig' %}

{% block body %}

	<h1 class="page-header">Edit location</h1>
	
	{{ form_start(form) }}
	
	<div class="form-group">
    <label class="col-sm-2 control-label">Calendar feed</label>
    <div class="col-sm-10">
      <p class="form-control-static">{{ url('apilocationcalendar', { 'id': location.id, 'key': app.user.apiKey }) }}</p>
    </div>
  </div>
	
	{{ form_widget(form) }}
	{{ form_end(form) }}
{% endblock %}